{
    "name": "eccube/eccube-upgrade-fixer",
    "type": "application",
    "description": "Eccube4.3に対応するようコードの変換を行います",
    "license": "MIT",
    "authors": [
        {
            "name": "Saša Stamenković",
            "email": "umpirsky@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "friendsofphp/php-cs-fixer": "^3.49.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.10"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Upgrade\\": "src/Symfony/Upgrade/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Upgrade\\Test\\": "tests/Symfony/Upgrade/Test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "bin": ["bin/symfony-upgrade-fixer"]
}
